#include <stdio.h>
#include <math.h>

void main ()
{
    int a, b=0, c=0;
    
    do
    {
        printf ("Dame el numero %i \n", b+1);
        scanf ("%i", &a);
        
        c = c + a;
        
        b = b + 1;
    }
    while (c <= 100 && b < 10);
    
    printf("Suma total: %i\n", c);

}
